{
  "name": "shopgun-sdk",
  "version": "2.2.18",
  "description": "ShopGun SDK for JavaScript.",
  "main": "dist/sgn-sdk.cjs.js",
  "browser": "dist/sgn-sdk.js",
  "module": "dist/sgn-sdk.es.js",
  "jsnext:main": "dist/sgn-sdk.es.js",
  "bundlesize": [
    {
      "path": "./dist/sgn-sdk.js",
      "maxSize": "400 kB"
    },
    {
      "path": "./dist/sgn-sdk.min.js",
      "maxSize": "60 kB"
    },
    {
      "path": "./dist/sgn-sdk.cjs.js",
      "maxSize": "35 kB"
    },
    {
      "path": "./dist/sgn-sdk.es.js",
      "maxSize": "35 kB"
    },
    {
      "path": "./kits/events/tracker.*js",
      "maxSize": "20 kB"
    }
  ],
  "scripts": {
    "test": "npm run test:lint && npm run test:ci && npm run test:size",
    "test:lint": "coffeelint lib/coffeescript __tests__",
    "test:ci": "run-s -l -c test:node test:jsdom test:chrome",
    "test:node": "jest --ci --env=node",
    "test:jsdom": "jest --ci --env=jsdom",
    "test:chrome": "jest --ci -c jest-chrome.json",
    "test:size": "bundlesize",
    "build:js": "rollup -c -m --environment NODE_ENV:production",
    "build:css": "stylus lib/stylus/sgn.styl -o dist/sgn-sdk.css --include-css -m",
    "build:css-min": "stylus lib/stylus/sgn.styl -o dist/sgn-sdk.min.css --include-css -m -c",
    "build": "run-p -l build:js build:css build:css-min",
    "dev:js": "rollup -c -m inline --no-treeshake --environment NODE_ENV:development --watch",
    "dev:css": "stylus lib/stylus/sgn.styl -o dist/sgn-sdk.css --include-css --sourcemap-inline --watch",
    "dev": "run-p -l dev:js dev:css serve",
    "serve": "serve",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "coffee",
      "litcoffee",
      "coffee.md"
    ],
    "transform": {
      "coffee|litcoffee|coffee.md": "./jest-coffeescript"
    },
    "testRegex": "__tests__?/.*\\.(coffee|litcoffee|coffee.md)$",
    "testPathIgnorePatterns": [
      "__tests__?/chrome/.*\\.(coffee|litcoffee|coffee.md)$",
      "node_modules",
      "fixtures"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shopgun/shopgun-js-sdk.git"
  },
  "author": "Morten Bo RÃ¸nsholdt <mr@shopgun.com> (https://shopgun.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shopgun/shopgun-js-sdk/issues"
  },
  "homepage": "https://github.com/shopgun/shopgun-js-sdk#readme",
  "dependencies": {
    "core-js": "^2.6.0",
    "cross-fetch": "^3.0.0",
    "incito-browser": "1.1.12",
    "md5": "^2.2.1",
    "microevent": "^1.0.0",
    "mustache": "^3.0.1",
    "rollup-plugin-terser": "^3.0.0",
    "sha256": "^0.2.0",
    "verso-browser": "^1.0.22"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "app-root-path": "^2.1.0",
    "aws-sdk": "^2.378.0",
    "bundlesize": "^0.17.0",
    "coffeelint": "^2.1.0",
    "coffeescript": "^2.3.2",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-puppeteer-preset": "^3.7.0",
    "memory-fs": "^0.4.1",
    "nib": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "process": "^0.11.10",
    "puppeteer": "^1.11.0",
    "rollup": "^0.68.0",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-coffee-script": "^2.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-string": "^2.0.2",
    "serve": "^10.1.1",
    "stylus": "^0.54.5",
    "terser": "^3.16.1",
    "thenify": "^3.3.0",
    "webpack": "^4.29.3"
  }
}
